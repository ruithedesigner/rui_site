<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= current_page.data.title || "Middleman" %></title>

		<link rel="apple-touch-icon" href="apple-touch-icon.png">
		
    <%= stylesheet_link_tag "site" %>

  <script>
    paceOptions = {
    initialRate:0.2,
    minTime: 2000,
    maxProgressPerFrame:1,
    ghostTime: 600
  }
  </script>


    <%= javascript_include_tag "pace.min" %>

    <script>
      function imgLoaded( el ){
        el.parentNode.parentNode.classList.add('loaded');
      }


    </script>

  </head>
  <body>

    <%= partial "menu" %>

    <div class="content-wrap">
      <%= yield %>
    </div>

    <%= partial 'images/symbol-defs' %>

    <%#= javascript_include_tag "TweenLite.min" %>
    <%#= javascript_include_tag "CSSPlugin.min" %>

    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <%#= javascript_include_tag "vimeo" %>    

    <%= stylesheet_link_tag "featherlight.min" %>
    <%= javascript_include_tag "featherlight" %>

    <%= javascript_include_tag "site" %>
  
  </body>
</html>
